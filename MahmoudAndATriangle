#include <iostream>
#include <algorithm>
using namespace std;


                    /*
                            Solution to http://codeforces.com/problemset/problem/766/B
                    */
int main()
{
    int s;
    cin >> s;
    int arr[s];
    for(int i=0;i<s;i++)
    {
        cin >> arr[i];
    }
    sort(arr,arr+s);
    for(int i=s-1-2;i>-1;i--)
    {
        if(arr[i] + arr[i+1] >arr[i+2])
        {
            cout << "YES" << endl;
            return 0;
        }
    }

    cout << "NO" << endl;
    return 0;
}
